{
  "name": "@xoxno/sdk-js",
  "version": "0.1.132-alpha",
  "description": "The SDK to interact with the XOXNO Protocol!",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./interactor": {
      "import": "./dist/interactor.esm.js",
      "types": "./dist/interactor.d.ts"
    },
    "./email": {
      "import": "./dist/email.esm.js",
      "types": "./dist/email.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.esm.js",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && webpack && webpack --config webpack-esm.config.cjs",
    "lint": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "commit": "git-cz",
    "release": "semantic-release",
    "docs": "typedoc --out docs src/**/*.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XOXNO/sdk-js.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx --no-install commitlint --edit \"$1\"",
      "pre-commit": "npm run format && npm run lint && npm run docs",
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "XOXNO",
    "MultiversX",
    "NFTs",
    "Blockchain",
    "Crypto"
  ],
  "author": "Mihai Eremia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/XOXNO/sdk-js/issues"
  },
  "homepage": "https://github.com/XOXNO/sdk-js#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.24",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "semantic-release": "^24.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@multiversx/sdk-core": "^13.2.1",
    "@multiversx/sdk-network-providers": "^2.4.3",
    "@react-email/components": "0.0.19",
    "@react-email/render": "^0.0.15",
    "next-intl": "^3.15.2",
    "react": "^18.3.1",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
